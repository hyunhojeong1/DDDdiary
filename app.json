{
  "name": "DDDdiary",
  "displayName": "DDDdiary",
  "expo": {
    "name": "DDDdiary",
    "slug": "DDDdiary",
    "scheme": "ddddiary",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/app-mainIcon-all.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": false,
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/images/app-mainIcon-all.png",
      "googleServicesFile": "./google-services.json",
      "package": "com.dolko.DDDdiary",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-mainIcon-all.png",
        "backgroundColor": "#021325"
      },
      "allowBackup": false,
      "versionCode": 1
    },
    "ios": {
      "icon": "./assets/images/app-mainIcon-ios.png",
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.dolko.DDDdiary",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "buildNumber": "1"
    },
    "web": {
      "favicon": "./assets/images/notification_icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/app-check",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "extraProguardRules": "-keep class com.google.android.gms.internal.consent_sdk.** { *; }"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification_icon.png"
        }
      ],
      [
